<nav>
	<ul style:"display:inline;">
		<li><button><%= link_to 'All', notes_path %></button></li>
		<li><button><%= link_to 'Home', new_note_path %></button></li>
	</ul>
</nav>
<h1>New Note</h1>
<%= form_for @note do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :body %>
  <%= f.text_area :body %>

  <%= f.submit %>
<% end %>

<%= link_to 'Back', notes_path %>